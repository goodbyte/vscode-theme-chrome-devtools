    "welcomePage.progress.foreground": "#7cacf8",
    "widget.border": "#313131"
  },
  "tokenColors": [
    {
      "scope": [
        "meta.embedded",
        // CSS
        "entity.name.tag.wildcard",
        "meta.property-list variable", // var(>--accent<)
        "punctuation.definition.entity", // >:<root
        "punctuation.section.function", // CSS function >()<
        "punctuation.separator", // CSS function >,<
        //
        "keyword.operator",
        "meta.function-call variable.other.object", // >console<.log()
        "punctuation.definition.template-expression",
        "storage.type.function.arrow",
        "variable.other.object" // >videos<.length
       ],
      "settings": {
        "foreground": "#e3e3e3"
      }
    }
  ]
